{
  "name": "@easy-team/res",
  "version": "2.0.1",
  "description": "A Powerful, Simple Node React Web Framework",
  "dependencies": {
    "egg": "^2.3.0",
    "egg-cors": "^2.1.1",
    "egg-scripts": "^2.10.0",
    "egg-view-nunjucks": "^2.2.0",
    "egg-view-react-ssr": "^3.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "conventional-changelog-cli": "^1.3.5",
    "egg-bin": "^4.4.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.18.0",
    "eslint-config-egg": "^7.0.0",
    "react-hot-loader": "^4.12.15",
